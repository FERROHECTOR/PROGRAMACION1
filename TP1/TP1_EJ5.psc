Algoritmo RAIZ_AB
	
	Definir A Como ENTERO;
	Definir B Como ENTERO;
	Definir RZ Como REAL;
	
	//Ingreso de Datos
	
	Escribir"Ingrese el índice de la raiz";
	Leer A;
	Escribir"Ingrese el radicando";
	Leer B;
	
	//Operacion Raiz de A, sobre B 
	
	RZ= B^(1/A);
	
	Escribir " El  resultado de la Operacion RAÍZ es  ",RZ;
	
FinAlgoritmo
